<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:pro="http://www.liquibase.org/xml/ns/pro"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd
        http://www.liquibase.org/xml/ns/pro
        http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.2.xsd " >


   <includeAll path="main/100_ddl" relativeToChangelogFile="true"/>
   <includeAll path="main/200_dml" relativeToChangelogFile="true"/>


    
    <changeSet author="amalik" id="insert_data" runOnChange="true" runAlways="true">
        <sqlFile path="main/PLS/insert_data.sql"
            relativeToChangelogFile="true"
            endDelimiter="/"
            stripComments="false"
            />
        <rollback>
            <sql>
                 DROP PROCEDURE insert_data;
            </sql>
        </rollback>
    </changeSet>


</databaseChangeLog>
